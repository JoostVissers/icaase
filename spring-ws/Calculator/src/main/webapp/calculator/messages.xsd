<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
        targetNamespace="http://www.han.nl/schemas/messages"
        xmlns:tns="http://www.han.nl/schemas/messages"
        xmlns:types="http://www.han.nl/schemas/types"
        elementFormDefault="qualified">

    <import namespace="http://www.han.nl/schemas/types" schemaLocation="types.xsd"/>
 
	<element name="CalculateRequest"> <!-- SubscriptionRequest -->
        <complexType>
            <sequence>
                <element name="input" type="types:CalculateInput" minOccurs="1" maxOccurs="1"/> <!-- SubscriptionUnput, met daarin user data,gekozen betaalmethode en ? -->
            </sequence>
        </complexType>	
	</element>

	<element name="CalculateResponse"> <!-- SubscriptionResponse -->
        <complexType>
            <sequence>
                <element name="result" type="types:CalculateResult" minOccurs="1" maxOccurs="1"/> <!-- SubscriptionResult, met een bericht ok of niet en gekozen subscription methode? -->
            </sequence>
        </complexType>	
	</element>
</schema>